  "version": "0.2.0",
  "configurations": [
    {
      "type": "cppdbg",
      "name": "Debug application",
      "request": "launch",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "setupCommands": [
        {
          "description": "Enable pretty-printing",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "program": "${workspaceFolder}/build/Debug/logservice/bin/logger_app",
      "linux": {
        "targetArchitecture": "x86_64",
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb"
      },
